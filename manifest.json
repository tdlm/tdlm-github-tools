{
    "name": "TDLM GitHub Tools",
    "author": "Scott Weaver",
    "homepage_url": "http://scottmw.com/",
    "manifest_version": 2,
    "version": "1.0.0",
    "description": "A few tools for GitHub",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "page_action": {},
    "content_scripts": [
        {
            "js": [
                "jquery.min.js",
                "background.js"
            ],
            "css": [
              "background.css"
            ],
            "matches": [
                "http://github.com/*",
                "https://github.com/*"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "https://github.com/*"
    ]
}